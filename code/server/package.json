        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-spellcheck": "^0.0.20",
        "globals": "^15.14.0",
        "nodemon": "^3.1.9",
        "prettier": "3.4.2",
