{
    "name": "stripe-billing-ilt",
    "version": "0.0.1",
    "description": "Stripe Billing ILT",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "lint": "npx eslint",
        "prestart": "npm run lint",
        "build": "tsc",
        "startServerDev": "tsx watch server.ts",
        "start": "npx tsx server.ts",
        "startListener": "stripe listen --forward-to http://localhost:4242/webhook",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "concurrently -i --kill-others \"npm run startServerDev\" \"npm run startListener\""
    },
    "author": "stripe-ecosystem",
    "license": "ISC",
    "dependencies": {
        "@babel/eslint-parser": "^7.27.0",
        "@types/cors": "^2.8.17",
        "@types/express-session": "^1.18.1",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-async-handler": "^1.2.0",
        "express-session": "^1.18.1",
        "express-validator": "^7.2.1",
        "lodash": "^4.17.21",
        "lorem-ipsum": "^2.0.8",
        "lowdb": "^7.0.1",
        "node-cache": "^5.1.2",
        "openai": "^4.94.0",
        "stripe": "^18.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^22.13.0",
        "concurrently": "^9.1.2",
